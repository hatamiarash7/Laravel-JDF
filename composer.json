{
  "name": "hatamiarash7/jdf",
  "description": "JDF Library for Laravel",
  "type": "library",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Arash Hatami",
      "email": "hatamiarash7@gmail.com",
      "homepage": "https://github.com/hatamiarash7"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.0",
    "illuminate/support": "^5.5|^6|^7"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "orchestra/testbench": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Hatamiarash7\\JDF\\": "src"
    },
    "classmap": [
      "src"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Hatamiarash7\\JDF\\Test\\": "tests"
    }
  },
  "suggest": {
    "laravel/framework": "Required for testing (~5.8).",
    "phpunit/phpunit": "Allow to use PHPUnit for testing (~7.0)."
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hatamiarash7\\JDF\\JDFServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "support": {
    "issues": "https://github.com/hatamiarash7/Laravel-JDF/issues"
  }
}
